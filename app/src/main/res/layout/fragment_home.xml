<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.HomeFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <!--Upperbar-->
        <FrameLayout
            android:id="@+id/frame_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/turquoise">

            <!--        <ImageView-->
            <!--            android:id="@+id/imageView11"-->
            <!--            android:layout_width="413dp"-->
            <!--            android:layout_height="255dp"-->
            <!--            app:layout_constraintEnd_toEndOf="parent"-->
            <!--            app:layout_constraintHorizontal_bias="0.6"-->
            <!--            app:layout_constraintStart_toStartOf="parent"-->
            <!--            app:layout_constraintTop_toTopOf="parent"-->
            <!--            app:srcCompat="@drawable/ico_upper_background" />-->

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/shapewhite" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="244dp"
                android:layout_height="86dp"
                android:text="Book \nAppointment"
                android:layout_gravity="start"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="60dp"
                android:textSize="28sp"
                android:textStyle="bold"
                android:fontFamily="sans-serif-condensed"
                app:layout_constraintBottom_toBottomOf="@+id/imageView11"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.168"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.38" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/bookButton"
                android:layout_width="170dp"
                android:layout_height="51dp"
                android:background="@drawable/textfield_background"
                android:text="Book Appointment"
                android:fontFamily="sans-serif-condensed"
                android:layout_gravity="bottom"
                android:layout_marginBottom="40dp"
                android:layout_marginLeft="15dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView11"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.131"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView8"
                app:layout_constraintVertical_bias="0.24" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="190dp"
                android:layout_height="136dp"
                android:layout_gravity="end"
                android:layout_marginTop="110dp"
                android:layout_marginRight="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView11"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.939"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0"
                app:srcCompat="@drawable/ico_appointment_user" />

        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingTop="10dp"
                    android:paddingLeft="12dp"
                    android:text="Dental Services:"
                    android:fontFamily="sans-serif-condensed"
                    android:textSize="20dp"
                    android:textStyle="bold"/>

                <!--Procedure Details Card-->
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="95dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="center_horizontal"
                    android:background="@drawable/ico_appointment_background"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="5dp">

                        <ImageView
                            android:layout_width="100dp"
                            android:layout_height="match_parent"
                            android:layout_margin="2dp"
                            android:scaleType="centerCrop"
                            android:adjustViewBounds="true"
                            android:background="@drawable/dental_crown"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:padding="3dp"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:layout_margin="2dp"
                            android:background="@drawable/profilecard_background">

                            <!--Display Name-->
                            <TextView
                                android:padding="3dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/dentalProd1"
                                android:textSize="18sp"
                                android:fontFamily="sans-serif-condensed"
                                android:textAlignment="center"
                                android:textColor="@color/black"/>

                            <!--Display Details-->

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                <!--Procedure Details Card-->
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="95dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="center_horizontal"
                    android:background="@drawable/ico_appointment_background"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="5dp">

                        <ImageView
                            android:layout_width="100dp"
                            android:layout_height="match_parent"
                            android:layout_margin="2dp"
                            android:scaleType="centerCrop"
                            android:adjustViewBounds="true"
                            android:background="@drawable/braces"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:padding="3dp"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:layout_margin="2dp"
                            android:background="@drawable/profilecard_background">

                            <!--Display Name-->
                            <TextView
                                android:padding="3dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/dentalProd2"
                                android:textSize="18sp"
                                android:fontFamily="sans-serif-condensed"
                                android:textColor="@color/black"
                                android:textAlignment="center"/>

                            <!--Display Details-->

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                <!--Procedure Details Card-->
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="95dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="center_horizontal"
                    android:background="@drawable/ico_appointment_background"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="5dp">

                        <ImageView
                            android:layout_width="100dp"
                            android:layout_height="match_parent"
                            android:layout_margin="2dp"
                            android:scaleType="centerCrop"
                            android:adjustViewBounds="true"
                            android:background="@drawable/oral"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:padding="3dp"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:layout_margin="2dp"
                            android:background="@drawable/profilecard_background">

                            <!--Display Name-->
                            <TextView
                                android:padding="3dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/dentalProd3"
                                android:textSize="18sp"
                                android:fontFamily="sans-serif-condensed"
                                android:textColor="@color/black"
                                android:textAlignment="center"/>

                            <!--Display Details-->

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                <!--Procedure Details Card-->
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="95dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="center_horizontal"
                    android:background="@drawable/ico_appointment_background"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="5dp">

                        <ImageView
                            android:layout_width="100dp"
                            android:layout_height="match_parent"
                            android:layout_margin="2dp"
                            android:scaleType="centerCrop"
                            android:adjustViewBounds="true"
                            android:background="@drawable/filling"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:padding="3dp"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:layout_margin="2dp"
                            android:background="@drawable/profilecard_background">

                            <!--Display Name-->
                            <TextView
                                android:padding="3dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/dentalProd4"
                                android:textSize="18sp"
                                android:fontFamily="sans-serif-condensed"
                                android:textColor="@color/black"
                                android:textAlignment="center"/>

                            <!--Display Details-->

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>

        </FrameLayout>



    </LinearLayout>
</FrameLayout>